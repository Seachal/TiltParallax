<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ParallaxThreeLayerView">
        <attr name="image_background" format="reference" />
        <attr name="parallax_intensity_background" format="float" />
        <attr name="tilt_sensitivity_background" format="float" />
        <attr name="forward_tilt_offset_background" format="float" />
        <attr name="max_translation_background" format="float" />
        <attr name="scaled_intensity_background" format="boolean" />

        <attr name="image_middleground" format="reference" />
        <attr name="parallax_intensity_middleground" format="float" />
        <attr name="tilt_sensitivity_middleground" format="float" />
        <attr name="forward_tilt_offset_middleground" format="float" />
        <attr name="max_translation_middleground" format="float" />
        <attr name="scaled_intensity_middleground" format="boolean" />

        <attr name="image_foreground" format="reference" />
        <attr name="parallax_intensity_foreground" format="float" />
        <attr name="tilt_sensitivity_foreground" format="float" />
        <attr name="forward_tilt_offset_foreground" format="float" />
        <attr name="max_translation_foreground" format="float" />
        <attr name="scaled_intensity_foreground" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ParallaxImageView">
        <attr name="parallax_intensity" format="float" />
        <attr name="tilt_sensitivity" format="float" />
        <attr name="forward_tilt_offset" format="float" />
        <attr name="max_translation" format="float" />
        <attr name="scaled_intensity" format="boolean" />
    </declare-styleable>

</resources>
